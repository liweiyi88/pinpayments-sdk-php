{
    "type": "library",
    "name": "liweiyi/pinpayments-sdk-php",
    "keywords": ["pin","payment","sdk"],
    "authors": [
        {
            "name": "Weiyi Li",
            "email": "weiyi.li713@gmail.com"
        }
    ],
    "license": "MIT",
    "description": "PHP SDK For Pin Payments",
